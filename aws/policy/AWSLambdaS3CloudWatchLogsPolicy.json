{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "logs:*"
            ],
            "Resource": [
                "arn:aws:logs:*:708478087677:log-group:/aws/lambda/s3-to-cloudwatch:log-stream:*",
                "arn:aws:logs:*:708478087677:log-group:/aws/lambda/s3-to-cloudwatch"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::my-iislogs/*"
        }
    ]
}